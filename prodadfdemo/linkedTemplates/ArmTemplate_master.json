{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "prodadfdemo"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"DlLs_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'DlLs'"
		},
		"bloclsforsink_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'bloclsforsink'"
		},
		"DlLs_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://demofordlsoc.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "prodadfdemo_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"DlLs_accountKey": {
						"value": "[parameters('DlLs_accountKey')]"
					},
					"bloclsforsink_connectionString": {
						"value": "[parameters('bloclsforsink_connectionString')]"
					},
					"DlLs_properties_typeProperties_url": {
						"value": "[parameters('DlLs_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}